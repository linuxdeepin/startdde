<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema path="/com/deepin/dde/startdde/" id="com.deepin.dde.startdde">
        <key type="i"  name="autostart-delay">
            <default>0</default>
            <summary>Autostart Delay Seconds</summary>
            <description>The delay seconds for autostart</description>
        </key>
        <key type="s"  name="wm-cmd">
            <default>''</default>
            <summary>The window manager start command</summary>
            <description>This key will special window manager at startdde startup. If empty, using deepin-wm.</description>
        </key>
        <key type="b"  name="launch-welcome">
            <default>true</default>
            <summary>Whether launch dde-welcome</summary>
            <description>This key will indicate whether showing dde-welcome.</description>
        </key>

        <key type="b" name="swap-sched-enabled">
            <default>false</default>
            <summary>swap sched enabled</summary>
            <description>deprecated, has no effect</description>
        </key>
        <key type="b" name="iowait-enabled">
            <default>false</default>
            <summary>iowait enabled</summary>
            <description></description>
        </key>
        <key type="b" name="memchecker-enabled">
            <default>false</default>
            <summary>memory checker enabled</summary>
            <description></description>
        </key>
        <key type="b" name="turbo-invoker-enabled">
            <default>false</default>
            <summary>deepin-turbo-invoker enabled</summary>
            <description></description>
        </key>
        <key type="b" name="quick-black-screen">
            <default>true</default>
            <summary>whether set DPMS after suspend and hibernate</summary>
            <description></description>
        </key>
        <key type="b" name="login-reminder">
            <default>false</default>
            <summary>show login reminder</summary>
            <description></description>
        </key>
    </schema>
</schemalist>
